<script>
    
    var veh = JSON.parse('@Json.Serialize(@Model.FormTDtl.Vechicles)');

    for (i = 0; i < veh.length; i++) {

        if (veh[i].vechicleType == "PC") {

            $(".tblPC > tbody  > tr > td:nth-child(" + ((veh[i].time / 5) + 1) + ")").html(veh[i].count)
        }
        else if (veh[i].vechicleType == "MC") {

            $(".tblMC > tbody  > tr > td:nth-child(" + ((veh[i].time / 5) + 1) + ")").html(veh[i].count)
        }
        else if (veh[i].vechicleType == "HV") {

            if (veh[i].loading == "2RE" || veh[i].loading == "3RE" || veh[i].loading == "3AE" || veh[i].loading == "4AE" || veh[i].loading == "5AE" || veh[i].loading == "6AE" || veh[i].loading == "7AE" ) {
                $(".tblHV > tbody  > ." + veh[i].loading + " > td:nth-child(" + ((veh[i].time / 5) + 3) + ")").html(veh[i].count)
            } else {
                $(".tblHV > tbody  > ." + veh[i].loading + " > td:nth-child(" + ((veh[i].time / 5) +1) + ")").html(veh[i].count)
            }
        }
    }

    CalSubTotal();
</script>