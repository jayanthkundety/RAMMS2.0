@model RAMMS.Web.UI.Models.FormTModel;
<div id="accordion" class="p-2 w-100 float-left">
    <div class="tab-content">
        <div class="tab-pane show active">

            <div class="row">
                <div class="col-12">
                    <div class="row">
                        <div class="form-group col-md-2">
                            @Html.Label("Date", "Date")
                            @Html.EditorFor(m => m.FormTDtl.InspectionDate, new { type = "date", htmlAttributes = new { @class = "form-control" } })
                        </div>
                        <div class="form-group col-md-4">
                            <div class="row">
                                <div class="col-6">
                                    <label for="Asset_ID">From Time </label>
                                    @Html.TextBoxFor(m => m.FormTDtl.AuditTimeFrm, new { @type = "text", @autocomplete = "off", @class = "form-control  validate {required,Time Taken From }", @onchange = "CalToTime()", @rows = 1 })
                                </div>
                                <div class="col-6">
                                    <label for="Asset_ID">To Time </label>
                                    @Html.TextBoxFor(m => m.FormTDtl.AuditTimeTo, new { @readonly = true, @type = "text", @autocomplete = "off", @class = "form-control  validate {required,Time Taken From }", @rows = 1 })
                                </div>
                            </div>

                        </div>
                        <div class="form-group col-md-2">
                            <label for="Asset_ID">
                                Hourly Count per Day
                            </label>
                            @Html.TextBoxFor(m => m.FormTDtl.HourlyCountPerDay, new { @type = "number", @class = "form-control  validate {required,Hourly Count PerDay}", @rows = 1 })
                        </div>
                        <div class="form-group col-md-2">
                            <label>Direction from</label>
                            @Html.TextBoxFor(m => m.FormTDtl.DirectionFrm, new { @type = "text", @class = "form-control  validate {required,Hourly Count PerDay}", @rows = 1 })
                        </div>
                        <div class="form-group col-md-2">
                            <label>Direction To</label>
                            @Html.TextBoxFor(m => m.FormTDtl.DirectionTo, new { @type = "text", @class = "form-control  validate {required,Hourly Count PerDay}", @rows = 1 })
                        </div>

                        <div class="form-group col-md-3">
                            <label for="Asset_ID">
                                Number of Day
                            </label>
                            <div class="row">
                                <div class="col-5 pr-0">
                                    @Html.TextBoxFor(m => m.FormTDtl.Day, new { @type = "number", @class = "form-control  validate {required,Hourly Count PerDay}", @rows = 1 })
                                </div>
                                <div class="col-1">
                                    <label class="pt-2"> of</label>
                                </div>
                                <div class="col-5 pl-0">
                                    @Html.TextBoxFor(m => m.FormTDtl.TotalDay, new { @type = "number", @class = "form-control  validate {required,Hourly Count PerDay}", @rows = 1 })
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-9">
                            <label>Description</label>
                            @Html.TextAreaFor(m => m.FormTDtl.Description, new { @class = "form-control  svalidate {required,Description}" })
                        </div>
                    </div>
